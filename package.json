    "@fastify/multipart": "9.2.1",
    "@fastify/sensible": "6.0.0",
    "@fastify/static": "8.2.0",
    "@fastify/swagger": "9.5.1",
    "@fastify/swagger-ui": "5.2.3",
    "@libsql/client": "0.15.15",
    "bcryptjs": "3.0.2",
    "drizzle-orm": "0.44.5",
